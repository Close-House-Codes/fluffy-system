//UTM Params function
function getUTMParameters() {
  const urlParams = new URLSearchParams(window.location.search);
  const utmParams = urlParams.get('utm_content');
  return utmParams;
}
var utmContent = getUTMParameters();

function logSession() {
  window.localStorage.otSession = Date.now();
}

function sendConsentReceipt(nbaEventDetail) {
  let onetrustID = window.OneTrust.getDataSubjectId();
  let consentStateAds = false;
  let consentStateAnalytics = false;
  let eventType = "Session Start";
  if (nbaEventDetail && typeof nbaEventDetail === "string") {
    eventType = nbaEventDetail;
  }
  if (window.OnetrustActiveGroups) {
    consentStateAds = window.OnetrustActiveGroups.indexOf("NBAad") > 0;
    consentStateAnalytics = window.OnetrustActiveGroups.indexOf("NBAmt") > 0;
  }

  let consentReceipt = {
    api_key: _satellite.getVar("Amplitude: OneTrust Key"),
    events: [
      {
        user_id: onetrustID,
        event_type: eventType,
        event_properties: {
          Webview: _satellite.getVar("Webview"),
        },
        user_properties: {
          "Consent Choice Made": window.OneTrust.IsAlertBoxClosed(),
          "Consent State Analytics": consentStateAnalytics,
          "Consent State Ads": consentStateAds,
        },
        app_version: document.querySelector("html").dataset.version,
        country: window._esi.country,
        time: Date.now(),
      },
    ],
  };


  
  
  fetch("https://api2.amplitude.com/2/httpapi", {
    method: "POST",
    headers: {
      "Content-Type": "application/json",
    },
    body: JSON.stringify(consentReceipt),
  })
    .then(function (res) {
      logSession();
    })
    .catch(function (error) {
      _satellite.logger.error("Fetch error:", error);
      return false;
    });
}

let oneTrustCheckCount = 1;

function oneTrustCheck() {
  if (window.OneTrust && typeof window.OneTrust.OnConsentChanged === "function") {
    window.OneTrust.OnConsentChanged(function () {
      sendConsentReceipt("Consent Update");
    });
    if (
      !window.localStorage["otSession"] ||
      window.localStorage["otSession"] < Date.now() - (30 * 60 * 1000)
    ) {
      _satellite.logger.info("needs consent receipt, sending to receipt writer")
      sendConsentReceipt();
    }
  } else if (oneTrustCheckCount < 13) {
    _satellite.logger.info("oneTrustCheck attempt", oneTrustCheckCount);
    oneTrustCheckCount++; 
    setTimeout(oneTrustCheck, 250);
  }
}

let consentManagerCheckCount = 1;

function consentManagerCheck() {
  if (window.nbaUserConsent && window.nbaUserConsent_initted) {
    if (typeof(window.Optanon) === "object") {
      _satellite.logger.info("CMP check passed, passing to onetrust")
      oneTrustCheck();
    } else if (consentManagerCheckCount < 13) {
      _satellite.logger.info(
        "consentManagerCheck attempt",
        consentManagerCheckCount
      );
      consentManagerCheckCount++;
      setTimeout(consentManagerCheck, 250);
    }
  } else{
    _satellite.logger.info("no CMP loaded, setting an event listener");
    document.addEventListener("userConsentReady", consentManagerCheck, false);
  }
}


consentManagerCheck();
